backend/[Dd]ebug/
backend/[Dd]ebugPublic/
backend/[Rr]elease/
backend/[Rr]eleases/
backend/x64/
backend/x86/
backend/[Ww][Ii][Nn]32/
backend/[Aa][Rr][Mm]/
backend/[Aa][Rr][Mm]64/
backend/bld/
backend/[Bb]in/
backend/[Oo]bj/
backend/[Ll]og/
backend/[Ll]ogs/

# .NET Core
backend/project.lock.json
backend/project.fragment.lock.json
backend/artifacts/

